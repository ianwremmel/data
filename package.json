    "@opentelemetry/api": "1.3.0"
  },
  "workspaces": [
    "."
  ],
  "types": "dist/types/runtime",
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
